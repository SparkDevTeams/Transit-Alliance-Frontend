        urlWhitelist.map( function(url) {
          if(ajmNewSiteURL.indexOf(url) > -1) {
            matchingList.push(url);
          }
        });


        // If the matchingList array contains a value then the link is whitelisted.
        if(matchingList.length > 0) {
